r\n    </div>\r\n  </div>\r\n\r\n  <script>\r\n  document.addEventListener('DOMContentLoaded', function() {\r\n    const modal = document.getElementById('modal');\r\n    const closeBtn = document.getElementById('close-modal');\r\n    const moreInfoBtn = document.getElementById('more-info-btn');\r\n    const fullModal = document.getElementById('full-info-modal');\r\n    const closeFullBtn = document.getElementById('close-full-modal');\r\n\r\n    // Show initial modal on load\r\n    modal.style.display = 'flex';\r\n    closeBtn.focus();\r\n\r\n    // Close initial modal\r\n    closeBtn.addEventListener('click', function() {\r\n      modal.style.display = 'none';\r\n    });\r\n\r\n    // Open full info modal\r\n    moreInfoBtn.addEventListener('click', function() {\r\n      modal.style.display = 'none';\r\n      fullModal.style.display = 'flex';\r\n      closeFullBtn.focus();\r\n    });\r\n\r\n    // Close full info modal\r\n    closeFullBtn.addEventListener('click', function() {\r\n      fullModal.style.display = 'none';\r\n    });\r\n\r\n    // Keyboard handling for initial modal\r\n    modal.addEventListener('keydown', function(e) {\r\n      if (e.key === 'Escape') {\r\n        modal.style.display = 'none';\r\n      }\r\n      if (e.key === 'Tab') {\r\n        e.preventDefault();\r\n        closeBtn.focus();\r\n      }\r\n    });\r\n\r\n    // Keyboard handling for full modal\r\n    fullModal.addEventListener('keydown', function(e) {\r\n      if (e.key === 'Escape') {\r\n        fullModal.style.display = 'none';\r\n      }\r\n      if (e.key === 'Tab') {\r\n        e.preventDefault();\r\n        closeFullBtn.focus();\r\n      }\r\n    });\r\n\r\n    // Close modals when clicking outside\r\n    modal.addEventListener('click', function(e) {\r\n      if (e.target === modal) {\r\n        modal.style.display = 'none';\r\n      }\r\n    });\r\n    fullModal.addEventListener('click', function(e) {\r\n      if (e.target === fullModal) {\r\n        fullModal.style.display = 'none';\r\n      }\r\n    });\r\n  });\r\n  </script>\r\n  <script src=\"fix-links.js\"></script>\r\n\r\n  <!-- Jubilee Popup -->\r\n  <div id=\"jubilee-popup\" class=\"jubilee-popup\" role=\"dialog\" aria-modal=\"true\">\r\n    <div class=\"jubilee-popup__inner\">\r\n      <button class=\"jubilee-popup__close\" id=\"jubilee-close\" aria-label=\"Sluiten\">‚úï</button>\r\n      <h2>10 jaar Spaanse Tuin & Zo!</h2>\r\n      <p>Vier ons jubileum met speciale acties. Bekijk de actiepagina voor meer informatie.</p>\r\n      <a href=\"opdrachten/ÔøΩÔøΩ 10 jaar Spaanse Tuin & Zo! ÔøΩÔøΩ\" class=\"jubilee-popup__btn\">Bekijk actie</a>\r\n    </div>\r\n  </div>\r\n  <script src=\"scripts/jubilee-popup.js\" defer></script>\r\n</body>\r\n</html>\r\n\r\n"
