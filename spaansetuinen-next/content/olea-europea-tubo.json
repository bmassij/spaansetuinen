u003cdiv class=\"w-full h-full bg-gradient-to-br from-emerald-700/30 to-emerald-900/30\"\u003e\u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \r\n    \u003cdiv class=\"relative z-10 max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\"\u003e\r\n      \u003cdiv class=\"grid md:grid-cols-2 gap-8 items-center\"\u003e\r\n        \r\n        \u003c!-- Links: Titel en beschrijving --\u003e\r\n        \u003cdiv\u003e\r\n          \u003ch1 class=\"text-4xl sm:text-5xl lg:text-6xl font-extrabold leading-tight mb-4\"\u003e\r\n            Olea Europea Tubo\r\n          \u003c/h1\u003e\r\n          \r\n          \u003cp class=\"text-lg sm:text-xl text-emerald-50 leading-relaxed\"\u003e\r\n            Wintervast tot -18 graden. Geef uw tuin een tropisch tintje met deze prachtige buisvormige olijfboom.\r\n          \u003c/p\u003e\r\n        \u003c/div\u003e\r\n        \r\n        \u003c!-- Rechts: Placeholder afbeelding --\u003e\r\n        \u003cdiv class=\"hidden md:flex justify-center items-center\"\u003e\r\n          \u003cdiv class=\"w-full max-w-md h-80 bg-white/10 backdrop-blur-sm rounded-2xl shadow-2xl flex items-center justify-center border-2 border-white/20\"\u003e\r\n            \u003cdiv class=\"text-center p-8\"\u003e\r\n              \u003cdiv class=\"text-6xl mb-4\"\u003eðŸŒ¿\u003c/div\u003e\r\n              \u003cp class=\"text-white/80 text-sm\"\u003eAfbeelding volgt\u003c/p\u003e\r\n            \u003c/div\u003e\r\n          \u003c/div\u003e\r\n        \u003c/div\u003e\r\n        \r\n      \u003c/div\u003e\r\n    \u003c/div\u003e\r\n  \u003c/header\u003e\r\n\r\n  \u003c!-- ========================================\r\n       CONTENT SECTION\r\n       ======================================== --\u003e\r\n  \u003cmain class=\"bg-white py-16\"\u003e\r\n    \u003carticle class=\"max-w-5xl mx-auto px-4 sm:px-6 lg:px-8\"\u003e\r\n      \r\n      \u003c!-- Breadcrumbs --\u003e\r\n      \u003cnav class=\"mb-8 text-sm text-gray-600\" aria-label=\"Breadcrumb\"\u003e\r\n        \u003col class=\"flex items-center space-x-2\"\u003e\r\n          \u003cli\u003e\u003ca href=\"index.html\" class=\"hover:text-emerald-600 transition\"\u003eHome\u003c/a\u003e\u003c/li\u003e\r\n          \u003cli\u003e\u003cspan class=\"text-gray-400\"\u003e/\u003c/span\u003e\u003c/li\u003e\r\n          \u003cli\u003e\u003ca href=\"index.html#onze-bomen\" class=\"hover:text-emerald-600 transition\"\u003eOnze bomen\u003c/a\u003e\u003c/li\u003e\r\n          \u003cli\u003e\u003cspan class=\"text-gray-400\"\u003e/\u003c/span\u003e\u003c/li\u003e\r\n          \u003cli\u003e\u003ca href=\"#\" class=\"hover:text-emerald-600 transition\"\u003eOlijfbomen\u003c/a\u003e\u003c/li\u003e\r\n          \u003cli\u003e\u003cspan class=\"text-gray-400\"\u003e/\u003c/span\u003e\u003c/li\u003e\r\n          \u003cli class=\"text-gray-900 font-medium\"\u003eOlea Europea Tubo\u003c/li\u003e\r\n        \u003c/ol\u003e\r\n      \u003c/nav\u003e\r\n\r\n      \u003c!-- ==================== INLEIDING ==================== --\u003e\r\n      \u003csection class=\"prose prose-lg max-w-none mb-12\"\u003e\r\n        \u003cp class=\"text-xl text-gray-700 leading-relaxed\"\u003e\r\n          De Olea Europea Tubo heeft een unieke cilindrische vorm. Een moderne variant voor hedendaagse tuinen.\r\n        \u003c/p\u003e\r\n        \u003cp class=\"text-lg text-gray-600 leading-relaxed mt-4\"\u003e\r\n          De boom groeit langzaam en bereikt een hoogte van ongeveer 4-6 meter. Hij is winterhard tot -18Â°C en heeft een sterke, robuuste uitstraling.\r\n        \u003c/p\u003e\r\n      \u003c/section\u003e\r\n\r\n      \u003c!-- ==================== KENMERKEN ==================== --\u003e\r\n      \u003csection class=\"mb-16\"\u003e\r\n        \u003ch2 class=\"text-3xl font-bold text-gray-900 mb-6 border-l-4 border-emerald-600 pl-4\"\u003e\r\n          Kenmerken\r\n        \u003c/h2\u003e\r\n        \r\n        \u003cdiv class=\"grid md:grid-cols-2 gap-8 items-start\"\u003e\r\n          \u003c!-- Tekst kolom --\u003e\r\n          \u003cdiv class=\"space-y-4 text-gray-700 leading-relaxed\"\u003e\r\n            \u003cp\u003e\r\n              Deze Olea Europea Tubo heeft een kenmerkende cilindrische vorm die met de jaren groeit. De bladeren zijn grijsgroen en de stam ontwikkelt een karakteristieke uitstraling.\r\n            \u003c/p\u003e\r\n            \u003cp\u003e\r\n              De boom gedijt het beste in volle zon en goed doorlatende grond. Hij kan zowel in de volle grond als in grote potten worden geplant.\r\n            \u003c/p\u003e\r\n            \u003cp\u003e\r\n              In het voorjaar verschijnen kleine witte bloemen die een aangename geur verspreiden, gevolgd door donkerblauwe olijven.\r\n            \u003c/p\u003e\r\n            \r\n            \u003c!-- Belangrijkste eigenschappen --\u003e\r\n            \u003cdiv class=\"bg-emerald-50 rounded-lg p-4 mt-6\"\u003e\r\n              \u003ch4 class=\"font-semibold text-emerald-800 mb-3\"\u003eBelangrijkste eigenschappen:\u003c/h4\u003e\r\n              \u003cul class=\"space-y-2\"\u003e\r\n                \u003cli class=\"flex items-start\"\u003e\r\n                  \u003cspan class=\"text-emerald-600 mr-2 mt-1\"\u003eâœ“\u003c/span\u003e\r\n                  \u003cspan\u003e\u003cstrong\u003eWinterhard tot -18 Â°C\u003c/strong\u003e\u003c/span\u003e\r\n                \u003c/li\u003e\r\n                \u003cli class=\"flex items-start\"\u003e\r\n                  \u003cspan class=\"text-emerald-600 mr-2 mt-1\"\u003eâœ“\u003c/span\u003e\r\n                  \u003cspan\u003e\u003cstrong\u003eUnieke cilindrische vorm\u003c/strong\u003e\u003c/span\u003e\r\n                \u003c/li\u003e\r\n                \u003cli class=\"flex items-start\"\u003e\r\n                  \u003cspan class=\"text-emerald-600 mr-2 mt-1\"\u003eâœ“\u003c/span\u003e\r\n                  \u003cspan\u003e\u003cstrong\u003eModerne uitstraling\u003c/strong\u003e\u003c/span\u003e\r\n                \u003c/li\u003e\r\n                \u003cli class=\"flex items-start\"\u003e\r\n                  \u003cspan class=\"text-emerald-600 mr-2 mt-1\"\u003eâœ“\u003c/span\u003e\r\n                  \u003cspan\u003e\u003cstrong\u003eLangzaam groeiend\u003c/strong\u003e\u003c/span\u003e\r\n                \u003c/li\u003e\r\n                \u003cli class=\"flex items-start\"\u003e\r\n                  \u003cspan class=\"text-emerald-600 mr-2 mt-1\"\u003eâœ“\u003c/span\u003e\r\n                  \u003cspan\u003e\u003cstrong\u003eMaximale hoogte: 6 meter\u003c/strong\u003e\u003c/span\u003e\r\n                \u003c/li\u003e\r\n              \u003c/ul\u003e\r\n            \u003c/div\u003e\r\n          \u003c/div\u003e\r\n          \r\n          \u003c!-- Afbeelding kolom - placeholder --\u003e\r\n          \u003cdiv class=\"flex justify-center\"\u003e\r\n            \u003cdiv class=\"w-full h-96 bg-gradient-to-br from-emerald-50 to-emerald-100 rounded-lg shadow-lg flex items-center justify-center border-2 border-emerald-200\"\u003e\r\n              \u003cdiv class=\"text-center p-8\"\u003e\r\n                \u003cdiv class=\"text-8xl mb-4\"\u003eðŸŒ¿\u003c/div\u003e\r\n                \u003cp class=\"text-emerald-700 font-medium\"\u003eOlea Europea Tubo\u003c/p\u003e\r\n                \u003cp class=\"text-emerald-600 text-sm mt-2\"\u003eAfbeelding placeholder\u003c/p\u003e\r\n              \u003c/div\u003e\r\n            \u003c/div\u003e\r\n          \u003c/div\u003e\r\n        \u003c/div\u003e\r\n      \u003c/section\u003e\r\n\r\n      \u003c!-- ==================== BLOEI EN VERZORGING ==================== --\u003e\r\n      \u003csection class=\"mb-16 bg-emerald-50 rounded-2xl p-8\"\u003e\r\n        \u003ch2 class=\"text-3xl font-bold text-gray-900 mb-6 border-l-4 border-emerald-600 pl-4\"\u003e\r\n          Verzorging\r\n        \u003c/h2\u003e\r\n        \r\n        \u003cdiv class=\"space-y-6 text-gray-700 leading-relaxed\"\u003e\r\n          \r\n          \u003ch3 class=\"text-xl font-semibold text-gray-900 mt-8 mb-3\"\u003eðŸŒ¿ Snoeien:\u003c/h3\u003e\r\n          \u003cp\u003e\r\n            Snoei minimaal om de natuurlijke vorm te behouden. Verwijder alleen dode takken.\r\n          \u003c/p\u003e\r\n\r\n          \u003ch3 class=\"text-xl font-semibold text-gray-900 mt-8 mb-3\"\u003eâ„ï¸ Winterbescherming:\u003c/h3\u003e\r\n          \u003cp\u003e\r\n            Hoewel winterhard, bescherm jonge bomen tegen langdurige vorst.\r\n          \u003c/p\u003e\r\n\r\n          \u003ch3 class=\"text-xl font-semibold text-gray-900 mt-8 mb-3\"\u003eðŸ’§ Hydrateren:\u003c/h3\u003e\r\n          \u003cdiv class=\"space-y-4\"\u003e\r\n            \u003cdiv class=\"bg-blue-50 p-4 rounded-lg\"\u003e\r\n              \u003ch4 class=\"font-semibold text-blue-800 mb-2\"\u003eIn pot:\u003c/h4\u003e\r\n              \u003cul class=\"text-sm space-y-1\"\u003e\r\n                \u003cli\u003eâ€¢ Water regelmatig, maar voorkom wateroverlast.\u003c/li\u003e\r\n                \u003cli\u003eâ€¢ Gebruik potten met goede drainage.\u003c/li\u003e\r\n              \u003c/ul\u003e\r\n            \u003c/div\u003e\r\n            \u003cdiv class=\"bg-green-50 p-4 rounded-lg\"\u003e\r\n              \u003ch4 class=\"font-semibold text-green-800 mb-2\"\u003eIn volle grond:\u003c/h4\u003e\r\n              \u003cp class=\"text-sm\"\u003e\r\n                Water tijdens droge periodes, maar laat de grond tussen gietbeurten drogen.\r\n              \u003c/p\u003e\r\n            \u003c/div\u003e\r\n          \u003c/div\u003e\r\n\r\n          \u003ch3 class=\"text-xl font-semibold text-gray-900 mt-8 mb-3\"\u003eðŸŒ± Voeding:\u003c/h3\u003e\r\n          \u003cp\u003e\r\n            Geef twee keer per jaar mediterrane plantenvoeding voor optimale gezondheid.\r\n          \u003c/p\u003e\r\n        \u003c/div\u003e\r\n      \u003c/section\u003e\r\n\r\n      \u003c!-- ==================== PLAATSING - Grid layout ==================== --\u003e\r\n      \u003csection class=\"mb-16\"\u003e\r\n        \u003ch2 class=\"text-3xl font-bold text-gray-900 mb-6 border-l-4 border-emerald-600 pl-4\"\u003e\r\n          Plaatsing\r\n        \u003c/h2\u003e\r\n        \r\n        \u003cdiv class=\"space-y-6 text-gray-700 leading-relaxed\"\u003e\r\n          \u003ch3 class=\"text-xl font-semibold text-gray-900 mb-3\"\u003eðŸŒ± Volle grond\u003c/h3\u003e\r\n          \u003cp\u003e\r\n            Plant in goed doorlatende grond op een zonnige plek voor de beste resultaten.\r\n          \u003c/p\u003e\r\n\r\n          \u003ch3 class=\"text-xl font-semibold text-gray-900 mb-3\"\u003eðŸª´ Bloembak/Bloempot\u003c/h3\u003e\r\n            \u003cp\u003e\r\n              Geschikt voor grote potten waar de cilindrische vorm mooi uitkomt.\r\n            \u003c/p\u003e\r\n\r\n          \u003ch3 class=\"text-xl font-semibold text-gray-900 mb-3\"\u003eâ˜€ï¸ Locatie\u003c/h3\u003e\r\n          \u003cp\u003e\r\n            Volle zon voor optimale groei en vruchtzetting.\r\n          \u003c/p\u003e\r\n        \u003c/div\u003e\r\n      \u003c/section\u003e\r\n\r\n      \u003c!-- ==================== CALL TO ACTION ==================== --\u003e\r\n      \u003csection class=\"bg-gradient-to-r from-emerald-600 to-emerald-700 rounded-2xl p-8 sm:p-12 text-white text-center\"\u003e\r\n        \u003ch2 class=\"text-3xl font-bold mb-4\"\u003eInteresse in een Olea Europea Tubo?\u003c/h2\u003e\r\n        \u003cp class=\"text-lg text-emerald-50 mb-4 max-w-2xl mx-auto\"\u003e\r\n          Vanaf â‚¬135,- voor deze moderne cilindrische olijfboom.\r\n        \u003c/p\u003e\r\n        \u003cp class=\"text-lg text-emerald-50 mb-6 max-w-2xl mx-auto\"\u003e\r\n          Wilt u weten welke olijfboom het beste bij uw tuin past? Neem contact met ons op voor beschikbaarheid en persoonlijk advies. Wij denken met plezier met u mee!\r\n        \u003c/p\u003e\r\n        \u003cdiv class=\"flex flex-wrap justify-center gap-4\"\u003e\r\n          \u003ca href=\"tel:+31611929392\" class=\"inline-flex items-center px-6 py-3 bg-white text-emerald-600 font-semibold rounded-lg hover:bg-gray-100 transition transform hover:scale-105\"\u003e\r\n            ðŸ“ž +31 6 11929392\r\n          \u003c/a\u003e\r\n          \u003ca href=\"mailto:lisette@spaansetuin-enzo.nl\" class=\"inline-flex items-center px-6 py-3 bg-emerald-800 text-white font-semibold rounded-lg hover:bg-emerald-900 transition transform hover:scale-105\"\u003e\r\n            âœ‰ï¸ E-mail ons\r\n          \u003c/a\u003e\r\n        \u003c/div\u003e\r\n      \u003c/section\u003e\r\n\r\n    \u003c/article\u003e\r\n  \u003c/main\u003e\r\n\r\n  \u003c!-- ========================================\r\n       FOOTER\r\n       ======================================== --\u003e\r\n  \u003cfooter class=\"bg-gray-900 text-white py-16\"\u003e\r\n    \u003cdiv class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\"\u003e\r\n      \u003cdiv class=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-12 mb-12\"\u003e\r\n        \r\n        \u003c!-- Kolom 1: Bedrijfsinfo \u0026 Logo --\u003e\r\n        \u003cdiv\u003e\r\n          \u003cimg src=\"logo.png\" alt=\"Spaanse Tuin \u0026 Zo Logo\" class=\"h-16 w-auto mb-4\"\u003e\r\n          \u003ch3 class=\"text-xl font-bold mb-3\"\u003eSpaanse Tuin \u0026 Zo\u003c/h3\u003e\r\n          \u003cp class=\"text-gray-400 text-sm leading-relaxed\"\u003e\r\n            Waar Mediterrane sfeer en kwaliteit samenkomen.\r\n          \u003c/p\u003e\r\n        \u003c/div\u003e\r\n\r\n        \u003c!-- Kolom 2: Contactgegevens --\u003e\r\n        \u003cdiv\u003e\r\n          \u003ch4 class=\"text-lg font-semibold mb-4 text-emerald-400\"\u003eContact\u003c/h4\u003e\r\n          \u003cul class=\"space-y-3 text-gray-300 text-sm\"\u003e\r\n            \u003cli class=\"flex items-start\"\u003e\r\n              \u003cspan class=\"mr-2\" aria-hidden=\"true\"\u003eðŸ“\u003c/span\u003e\r\n              \u003caddress class=\"not-italic\"\u003e\r\n                Heinsbergerweg 20\u003cbr\u003e6074 AD Melick\r\n              \u003c/address\u003e\r\